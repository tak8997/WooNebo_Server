<%
    layout('/layout')
    stylesheet('/css/product.css')
    script('/js/product.js')
%>
<div class="form-edit">
    <form class="form form-horizontal" action="?method=PUT" method="POST" if-none-match=”*”>
        <div class="form-label form-group">
            <div class="col-sm-2"></div>
            <div class="col-sm-8">
                <h3 class="center">상품 정보 변경</h3>
            </div>
            <div class="col-sm-2"></div>
        </div>
        <%- include form %>
        <div class="form-group">
            <div class="col-sm-2"></div>
            <div class="col-sm-8">
                <button class="btn btn-info" type="submit">변경</button>
            </div>
            <div class="col-sm-2"></div>
        </div>
    </form>
</div>
